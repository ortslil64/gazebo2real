
<sdf version='1.6'>

  <world name='default'>
    <physics name='default_physics' default='0' type='ode'>
      <ode>
        <solver>
          <type>quick</type>
          <iters>40</iters>
          <precon_iters>0</precon_iters>
          <sor>1.3</sor>
          <use_dynamic_moi_rescaling>0</use_dynamic_moi_rescaling>
        </solver>
        <constraints>
          <cfm>0</cfm>
          <erp>0.2</erp>
          <contact_max_correcting_vel>100</contact_max_correcting_vel><!-- 100-->
          <contact_surface_layer>0.001</contact_surface_layer><!-- 0.001-->
        </constraints>
      </ode>
      <real_time_update_rate>500</real_time_update_rate>
      <max_step_size>0.002</max_step_size><!-- 0.005-->
      <!--<real_time_factor>1</real_time_factor>-->
    </physics>
    <light name='sun' type='directional'>
